#!/bin/sh
echo "Making pre-invocation changes..."
/usr/libexec/s2i/assemble
rc=$?
if [ $rc -eq 0 ]; then
 echo "Making post-invocation changes..." 
 echo '###### npm install -g angular-cli@1.0.0-beta.28.3'
 npm install -g angular-cli@1.0.0-beta.28.3
 echo '###### npm install -g angular-cli@1.0.0-beta.28.3 end'
else
 echo "Error: assemble failed!"
fi
exit $rc

